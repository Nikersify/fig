{
  "name": "figjs",
  "version": "0.0.0",
  "description": "front-end js framework",
  "main": "dist/fig.js",
  "author": "nikersify <nikersify@nikerino.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.25.0",
    "babel-preset-env": "^1.5.2",
    "big-eye": "^0.0.2",
    "browserify": "^14.4.0",
    "figify": "^0.0.0",
    "jsdom": "^11.0.0",
    "listr": "^0.12.0",
    "regenerator-runtime": "^0.10.5",
    "rollup": "^0.43.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^3.0.19"
  },
  "scripts": {
    "build": "node build/build.js",
    "build:watch": "big-eye 'npm run build' -w src/",
    "test": "xo && ava",
    "test:watch": "ava --watch"
  },
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "babel": {
    "presets": [
      "@ava/stage-4"
    ]
  },
  "xo": {
    "semicolon": false,
    "envs": [
      "browser"
    ],
    "rules": {
      "max-len": [
        "error",
        80
      ]
    }
  },
  "dependencies": {}
}
